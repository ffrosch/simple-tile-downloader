{
  "name": "simple-tile-downloader",
  "version": "0.2.0-alpha",
  "type": "module",
  "author": "Florian Frosch",
  "dependencies": {
    "lodash.partial": "^4.2.1",
    "ol": "^10.6.1"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "@types/lodash.partial": "^4.2.9"
  },
  "description": "Download XYZ tiles from a service given a bounding box and the target zoom levels.",
  "keywords": [
    "xyz",
    "tile",
    "download",
    "map",
    "promise"
  ],
  "license": "ISC",
  "scripts": {
    "build": "bun build ./src/index.ts --outfile=dist/index.js --target=browser --packages=external && bun run build:declaration",
    "build:declaration": "tsc --project tsconfig.types.json",
    "publish": "bun run build && bun publish"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ]
}
